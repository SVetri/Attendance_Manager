<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floating1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_margin="10dp"
        android:onClick="floatingclicked"
        app:elevation="10dp"
        app:borderWidth="0dp"
        app:backgroundTint="#3f85f4"

        />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="*"
            android:background="#ffffff">

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="8:30"
                    android:textSize="25sp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="1"/>

                <TextView
                    android:id="@+id/sub1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="Computer organization and  Architecture"
                    android:textSize="25sp"
                    android:padding="5dp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="2"/>

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="9:20"
                    android:textSize="25sp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="1"/>

                <TextView
                    android:id="@+id/sub2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="Computer Networks"
                    android:padding="5dp"
                    android:textSize="25sp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="2"/>

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="10:30"
                    android:textSize="25sp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="1"/>

                <TextView
                    android:id="@+id/sub3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="Algorithms"
                    android:padding="5dp"
                    android:textSize="25sp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="2"/>

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="11:20"
                    android:textSize="25sp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="1"/>

                <TextView
                    android:id="@+id/sub4"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="Principles of Programming Languages"
                    android:padding="5dp"
                    android:textSize="25sp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="2"/>

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="1:30"
                    android:textSize="25sp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:layout_margin="2dp"
                    android:layout_weight="1"/>

                <TextView
                    android:id="@+id/sub5"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
